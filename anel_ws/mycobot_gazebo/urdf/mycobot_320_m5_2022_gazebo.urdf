<?xml version="1.0"?>
<robot  xmlns:xacro="http://www.ros.org/wiki/xacro" name="mycobot_320" >
  <link name="world"/>
  <!-- <xacro:property name="width" value=".2" /> -->
  <xacro:property name="effort" value="5.0"/>
  <xacro:property name="velocity" value="2.792527"/>

  <link name="base_link">
    <visual>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/base.dae"/>
      </geometry>
      <origin xyz = "0.0 0 0 " rpy = " 0 0 ${pi/2}"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/base.dae"/>
      </geometry>
      <origin xyz = "0.0 0 0 " rpy = " 0 0 ${pi/2}"/>
    </collision>
		<inertial>
			<origin xyz="0 0 0.0" rpy="0 0 0"/>
			<mass value="0.33"/>
			<inertia
				ixx="0.000784" ixy="0.0" ixz="0.0"
				iyy="0.000867" iyz="0.0"
				izz="0.001598"/>
		</inertial>
  </link>

  <link name="link1">
    <visual>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link1.dae"/>
      </geometry>

    <origin xyz = "0.0 0.0 -0.078 " rpy = " 0 0 1.5708"/>
    </visual>
    <collision>
     <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link1.dae"/>
      </geometry>
     <origin xyz = "0.0 0.0 -0.078 " rpy = " 0 0 1.5708"/>
    </collision>
    
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.12"/>
      <inertia
        ixx="0.000148" ixy="0.0" ixz="0.0"
        iyy="0.000148" iyz="0.0"
        izz="0.000096"/>
    </inertial>
  </link>


  <link name="link2">
    <visual>
      <geometry>
       
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link2.dae"/>
      </geometry>
       <origin xyz = "0.0 0.0 -0.04 " rpy = " 0 1.5708 0"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link2.dae"/>
      </geometry>
       <origin xyz = "0.0 0.0 -0.04 " rpy = " 0 1.5708 0"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.19"/>
      <inertia
        ixx="0.000148" ixy="0.0" ixz="0.0"
        iyy="0.000148" iyz="0.0"
        izz="0.000096"/>
    </inertial>
  </link>


  <link name="link3">
    <visual>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link3.dae"/>
      </geometry>
    <origin xyz = "0.0 0.0 0.039 " rpy = " 1.5708 1.5708 1.5708"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link3.dae"/>
      </geometry>
      <origin xyz = "0.0 0.0 0.039 " rpy = " 1.5708 1.5708 1.5708"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.16"/>
      <inertia
        ixx="0.000148" ixy="0.0" ixz="0.0"
        iyy="0.000148" iyz="0.0"
        izz="0.000096"/>
    </inertial>
  </link>


  <link name="link4">
    <visual>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link4.dae"/>
      </geometry>
         <origin xyz = "0.0 -0.0024 -0.05" rpy = " 1.5708 1.5708 0"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link4.dae"/>
      </geometry>
      <origin xyz = "0.0 -0.0024 -0.05" rpy = " 1.5708 1.5708 0"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.124"/>
      <inertia
        ixx="0.000103" ixy="0.0" ixz="0.0"
        iyy="0.000103" iyz="0.0"
        izz="0.000096"/>
    </inertial>
  </link>


  <link name="link5">
    <visual>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link5.dae"/>
      </geometry>
    <origin xyz = "0.0 0.0 -0.05 " rpy = " 0  0 -1.5708"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link5.dae"/>
      </geometry>
      <origin xyz = "0.0 0.0 -0.05 " rpy = " 0  0 -1.5708"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.11"/>
      <inertia
        ixx="0.000103" ixy="0.0" ixz="0.0"
        iyy="0.000103" iyz="0.0"
        izz="0.000096"/>
    </inertial>
  </link>


  <link name="link6">
    <visual>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link6.dae"/>
      </geometry>
      <origin xyz = "0.0 0.0 -0.0115 " rpy = " 1.5708 1.5708 0"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="file://$(find mycobot_ros2_description)/mesh/link6.dae"/>
      </geometry>
      <origin xyz = "0.0 0.0 -0.0115 " rpy = " 1.5708 1.5708 0"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.0739"/>
      <inertia
        ixx="0.00006649" ixy="0.0" ixz="0.0"
        iyy="0.00006649" iyz="0.0"
        izz="0.000096"/>
    </inertial>
  </link>

  <joint name="virtual_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz= "0 0 0" rpy = "0 0 0"/>
  </joint>

  <joint name="joint2_to_joint1" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "${effort}" lower = "-2.87" upper = "2.87" velocity = "${velocity}"/>
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz= "0 0 0.173" rpy = "0 0 0"/> 
    <dynamics damping="0.05" friction="0.05"/>
  </joint>


  <joint name="joint3_to_joint2" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "${effort}" lower = "-2.87" upper = "2.87" velocity = "${velocity}"/>
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz= "0 -0.086 0" rpy = "0 -1.5708 1.5708"/> 
    <dynamics damping="0.205" friction="0.15"/>
  </joint>


  <joint name="joint4_to_joint3" type="revolute">
    <axis xyz=" 0 0 1"/>
    <limit effort = "${effort}" lower = "-2.879" upper = "2.879" velocity = "${velocity}"/>
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz= "0.13635 0 -0.086   " rpy = "0 0 0"/>
    <dynamics damping="0.21" friction="0.41"/>
  </joint>

  <joint name="joint5_to_joint4" type="revolute">
    <axis xyz=" 0 0 1"/>
    <limit effort = "${effort}" lower = "-2.879" upper = "2.879" velocity = "${velocity}"/>
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz = "0.1195 0 0.082 " rpy = " 0 0 1.57080"/>
    <dynamics damping="0.10" friction="0.20"/>
  </joint>

  <joint name="joint6_to_joint5" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "${effort}" lower = "-2.879" upper = "2.879" velocity = "${velocity}"/>
    <parent link="link4"/>
    <child link="link5"/>
     <origin xyz= "0 -0.09415 0.0" rpy = "1.5708 0 0"/>
     <dynamics damping="0.01" friction="0.005"/>
  </joint>

  <joint name="joint6output_to_joint6" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "${effort}" lower = "-3.05" upper = "3.05" velocity = "${velocity}"/>
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz= "0 0.055 0.0" rpy = "-1.5708 0 0"/>
  </joint>

  <gazebo>
    <plugin filename="ignition-gazebo-joint-state-publisher-system" name="ignition::gazebo::systems::JointStatePublisher">
      <topic>joint_states</topic>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin filename="ignition-gazebo-joint-position-controller-system" name="ignition::gazebo::systems::JointPositionController">
      <joint_name>joint2_to_joint1</joint_name>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin filename="ignition-gazebo-joint-position-controller-system" name="ignition::gazebo::systems::JointPositionController">
      <joint_name>joint3_to_joint2</joint_name>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin filename="ignition-gazebo-joint-position-controller-system" name="ignition::gazebo::systems::JointPositionController">
      <joint_name>joint4_to_joint3</joint_name>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin filename="ignition-gazebo-joint-position-controller-system" name="ignition::gazebo::systems::JointPositionController">
      <joint_name>joint5_to_joint4</joint_name>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin filename="ignition-gazebo-joint-position-controller-system" name="ignition::gazebo::systems::JointPositionController">
      <joint_name>joint6_to_joint5</joint_name>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin filename="ignition-gazebo-joint-position-controller-system" name="ignition::gazebo::systems::JointPositionController">
      <joint_name>joint6output_to_joint6</joint_name>
    </plugin>
  </gazebo>

